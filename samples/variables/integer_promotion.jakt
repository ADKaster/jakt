struct Foo {
    x: u16
}

function main() {
    let mutable a: u8 = 100
    a += 100

    let mutable b: u16 = 1000
    b += 1000

    let mutable c: u32 = 100000
    c += 100000

    let mutable f = Foo(x: 123)
    f.x += 2

    println("{}", a)
    println("{}", b)
    println("{}", c)
    println("{}", f.x)
}
